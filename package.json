    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml}\" && prettier-package-json --write"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "child-process-promise": "^2.2.1",
