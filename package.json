    "fast-glob": "^3.2.2",
    "gatsby": "^2.23.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-styled-components": "^3.3.3",
    "gatsby-plugin-typescript": "^2.4.4",
    "gray-matter": "^4.0.2",
    "marked": "^1.1.0",
    "md5-file": "^5.0.0",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-imgix": "^9.0.2",
